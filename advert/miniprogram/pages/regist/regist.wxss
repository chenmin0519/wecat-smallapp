
page {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.regist-title{
  text-align: center;
  font-size: 26px;
  padding-left: 90px;
  font: bold;
  font-family: FangSong;
}
.row-regist{
  margin-top: 20rpx;
  overflow: hidden;
  line-height: 100rpx;
  border-bottom: 1rpx solid #ccc;
  margin-left: 20rpx;
  margin-right: 20rpx;
  color: #777;
  background: #fff;
  display: flex;
}
.info-input-regist{
  height: 100rpx;
  margin-left: 50rpx;
  color: #777;
    float: left;
}
.info-input1-regist{
  height: 100rpx;
  margin-left: 50rpx;
  color: #777;
    float: left;
    width: 420rpx;
}
.button-regist{
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  background: #00ccff;
  float: left;
  margin-left: 10rpx;
  margin-top: 15rpx;
  color: #FFFFFF;
}
.submit-regist{
  margin-top: 50rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  background: #00CCFF;
   color: #FFFFFF;
}
.success-regist{
  background: #ffffff;
 
}
.cheer-regist{
  text-align: center;
  line-height: 400rpx;
  font-size: 60rpx;
  position: relative;
}
.return-regist{
  margin: 20rpx;
}

.err_text{
  color: #FF0000;
  font-family: FangSong;
}